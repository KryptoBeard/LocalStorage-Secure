<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RazorLangVersion>3.0</RazorLangVersion>
    <RootNamespace>Blazored.LocalStorage</RootNamespace>

    <PackageId>Blazored.LocalStorage.Secure</PackageId>
    <Version>1.0</Version>
    <Authors>Dylan</Authors>
    <Description>A library to provide access to local storage in Blazor applications, original by Chris Sainty</Description>
    <Copyright>Copyright 2020 (c) Chris Sainty. All rights reserved.</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/KryptoBeard/LocalStorage-Secure</PackageProjectUrl>
    <PackageIconUrl>https://avatars1.githubusercontent.com/u/39194025</PackageIconUrl>
    <RepositoryUrl>https://github.com/KryptoBeard/LocalStorage-Secure</RepositoryUrl>
    <PackageTags>Blazor LocalStorage Fork Blazored Razor Components</PackageTags>
    <Company />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components" Version="3.1.4" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="3.1.4" />
  </ItemGroup>

</Project>
